public class Person {
  private String name;
  private String contactNum;
  
  public String getName() {
    return name;
  }
  
  public String getContactNum() {
    return contactNum;
  }
  
  public void setName(String name) {
    this.name = name;
  }
  
  public void setContactNum(String contactNum) {
    this.contactNum = contactNum;
  }
} 
